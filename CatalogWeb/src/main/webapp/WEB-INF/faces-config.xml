<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="1.2"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.Main</base-name>
            <var>mainMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.SearchAuto</base-name>
            <var>searchAutoMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.CrossReference</base-name>
            <var>crossReferenceMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.GoodwillGood</base-name>
            <var>goodwillGoodMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.Filter</base-name>
            <var>filterMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.SearchSize</base-name>
            <var>searchSizeMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.Base</base-name>
            <var>baseMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>ru.goodfil.catalog.web.UnloadingByBrand</base-name>
            <var>unloadingMsg</var>
        </resource-bundle>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>ru</supported-locale>
            <supported-locale>fr</supported-locale>
        </locale-config>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
    </application>

    <managed-bean>
        <managed-bean-name>TabsBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.TabsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>SearchAutoBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.SearchAutoBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>UnloadingByBrandBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.UnloadingByBrandBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>CrossReferenceBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.CrossReferenceBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>GoodwillGoodBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.GoodwillGoodBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>FilterBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.FilterBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ShortDateTimeConverter</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.converter.ShortDateTimeConverter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>FilterTypeCodeConverter</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.converter.FilterTypeCodeConverter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>FilterFormConverter</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.converter.FilterFormConverter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>LanguageBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.LanguageBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>SearchSizeBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.SearchSizeBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>NumberValidator</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.validator.NumberValidator</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>SettingsBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.SettingsBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


    <managed-bean>
        <managed-bean-name>VersionBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.VersionBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>InjectorBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.InjectorBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>i18Bean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.i18n.I18ManagedBean</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>UpdaterBean</managed-bean-name>
        <managed-bean-class>ru.goodfil.catalog.web.UpdaterBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <validator>
        <validator-id>NumberValidator</validator-id>
        <validator-class>ru.goodfil.catalog.web.validator.NumberValidator</validator-class>
    </validator>
</faces-config>